{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/app/*": [
        "src/app/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/config/*": [
        "src/config/*"
      ],
    }
  }
}